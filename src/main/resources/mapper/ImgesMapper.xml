<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.shine.seimi.dao.ImgesMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="cn.shine.seimi.domain.Imges">
                    <id column="gid" property="gid"/>
                    <result column="number" property="number"/>
                    <result column="movie_pic_about" property="moviePicAbout"/>
                    <result column="movie_pic_detail" property="moviePicDetail"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            number, gid, movie_pic_about, movie_pic_detail
        </sql>


    <select id="selectPageVo" resultMap="BaseResultMap">
        SELECT * FROM javbooks_movie
    </select>
</mapper>